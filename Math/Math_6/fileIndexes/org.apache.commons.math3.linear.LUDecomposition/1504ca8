
















package org.apache.commons.math.linear;

import java.io.Serializable;




















public interface LUDecomposition extends Serializable {

    




    RealMatrix getL();

    




    RealMatrix getU();

    








    RealMatrix getP();

    




    int[] getPivot();

    



    boolean evenPermutation();

    



    boolean isSingular();

    



    double getDeterminant();

    



    DecompositionSolver getSolver();

}

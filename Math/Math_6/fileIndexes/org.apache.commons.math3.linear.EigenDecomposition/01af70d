
















package org.apache.commons.math.linear;

import java.io.Serializable;

























public interface EigenDecomposition extends Serializable {

    





    RealMatrix getV();

    






    RealMatrix getD();

    





    RealMatrix getVT();

    




    double[] getEigenvalues();

    





    double getEigenvalue(int i);

    





    RealVector getEigenvector(int i);

}






















































package org.joda.time.convert;

import org.joda.time.ReadWritableTimePeriod;








class LongConverter extends AbstractConverter implements InstantConverter, DurationConverter {
    
    


    static final LongConverter INSTANCE = new LongConverter();
    
    


    protected LongConverter() {
        super();
    }

    
    







    public long getInstantMillis(Object object) {
        return ((Long) object).longValue();
    }
    
    
    







    public long getDurationMillis(Object object) {
        return ((Long) object).longValue();
    }

    
    









    public void setInto(ReadWritableTimePeriod duration, Object object) {
        duration.setTimePeriod(((Long) object).longValue());
    }

    
    




    public Class getSupportedType() {
        return Long.class;
    }
    
}
